# World Series of Agents — copy to .env and fill in (no quotes around values)

# LLM (OpenAI-compatible) — get key at https://platform.openai.com/account/api-keys
OPENAI_API_KEY=sk-your-openai-key-here
OPENAI_API_BASE=

# Data / news — get key at https://www.alphavantage.co/support/#api-key
ALPHAADVANTAGE_API_KEY=your-alpha-vantage-key-here

# Optional: DeepSeek — enable deepseek-chat agents in league_config.json
# Get key at https://platform.deepseek.com/
DEEPSEEK_API_KEY=

# Optional: OpenRouter — enable Claude / Gemini agents in league_config.json
# Get key at https://openrouter.ai/keys
OPENROUTER_API_KEY=

# MCP service ports (optional)
MATH_HTTP_PORT=8000
SEARCH_HTTP_PORT=8001
TRADE_HTTP_PORT=8002
GETPRICE_HTTP_PORT=8003
CRYPTO_HTTP_PORT=8005

# Runtime config path (optional)
RUNTIME_ENV_PATH=./runtime_env.json
